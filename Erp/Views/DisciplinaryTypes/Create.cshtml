@model Erp.Models.DisciplinaryType

@{
    ViewData["Title"] = "Create";
}
<div class="container px-5">

    <div class="card">
        <div class="card-header border-bottom-0">
            <div class="card-title mb-5">
                <h5 class="text-muted text-">Disciplinary Type  </h5>
            </div>
        </div>
        <div class="card-body">
            <div class="card-body">
                <form asp-action="Create">
                    <div class="card-body">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <label asp-for="type" class="control-label">Punishment Type</label>
                            <input asp-for="type" class="form-control" placeholder="TYpe" />
                            <span asp-validation-for="type" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Describtion" class="control-label">Describtion </label>
                            <input asp-for="Describtion" class="form-control" placeholder="TYpe" />
                            <span asp-validation-for="Describtion" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="card-footer bg-white p-2">
                        <div class="row p-0 m-0">
                            <div class="col align-self-start pl-3">
                                <a asp-action="Index">Back to List</a>
                            </div>
                            <div class="col-2  align-self-end  p-0">
                                <div class="form-group mb-0">

                                    <input type="reset" value="Clear" class="btn btn-glow-dark btn-secondary  mr-3" />
                                    <input type="submit" value="Create" class="btn btn-glow-dark btn-primary ml-4" />

                                </div>
                            </div>
                        </div>
                    </div>
                   
     
                </form>
          </div>
            </div>
        </div>
</div>
