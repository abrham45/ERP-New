@model Erp.Models.Tax

@{
    ViewData["Title"] = "Details";
}

<div class="container px-5">
    <div class="card p-3">
        <div class="card-header border-bottom-0">
            <div class="card-title mb-5">
                <h5 class="text-muted text-">
                    Tax Details
                </h5>
            </div>
        </div>

        <div class="card-body">
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.MinAmount)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.MinAmount)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.MaxAmount)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.MaxAmount)
                </dd>
             
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TaxPercent)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TaxPercent)
                </dd>
            </dl>
        </div>
        <div class="card-footer">
            <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
            <a asp-action="Index">Back to List</a>
        </div>
    </div>
</div>
