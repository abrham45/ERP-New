@model Erp.Models.Asset

@{
    ViewData["Title"] = "Details";
}

<div class="container">
    <div class="card">
        <div class="card-header border-bottom-0">
            <div class="card-title mb-5">
                <h5 class="text-muted text-"> Asset Detail</h5>
            </div>
        </div>
        <div class="card-body">
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Asset_Name)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Asset_Name)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Asset_typeId)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Asset_typeId)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.factory_number)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.factory_number)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.serial_number)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.serial_number)
                </dd>

            </dl>
        </div>
        <div>
            <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
            <a asp-action="Index">Back to List</a>
        </div>
    </div>
</div>