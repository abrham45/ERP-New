@model Erp.Models.LateCome

@{
    ViewData["Title"] = "Create";
}

<div class="container px-5">

    <div class="card">
        <div class="card-header border-bottom-0">
            <div class="card-title mb-5">
                <h5 class="text-muted text-">
                    Asset Type Form
                </h5>
            </div>

        </div>

        <div class="card-body">
            <form asp-action="Create">
                <div class="card-body">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="MorningLate" class="control-label">Morning Late</label>
                        <input asp-for="MorningLate" class="form-control" />
                        <span asp-validation-for="MorningLate" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="AfternoonLate" class="control-label">AfternoonLate</label>
                        <input asp-for="AfternoonLate" class="form-control" />
                        <span asp-validation-for="AfternoonLate" class="text-danger"></span>
                    </div>

                </div>
                <div class="card-footer bg-white p-2">
                    <div class="row p-0 m-0">
                        <div class="col align-self-start pl-3">
                            <a asp-action="Index">Back to List</a>
                        </div>
                        <div class="col-2  align-self-end  p-0">
                            <div class="form-group mb-0">
                               
                                <input type="submit" value="Create" class="btn btn-glow-dark btn-primary ml-4" />

                            </div>
                        </div>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>



<hr />


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
